  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "example",
      "cwd": "openidconnect\\example",
      "request": "launch",
      "type": "dart",
      "args": [
        "--web-enable-expression-evaluation",
        "--disable-web-security",
        "--web-port",
        "62568"
      ]
    },
    {
      "name": "example chrome",
      "cwd": "openidconnect\\example",
      "request": "launch",
      "type": "dart",
      "args": [
        "-d",
        "chrome",
        "--web-port",
        "15503",
        "--web-browser-flag",
        "--disable-web-security",
        "--web-browser-flag",
        "--user-data-dir=%temp%/search"
      ]
    },
    {
      "name": "example (profile mode)",
      "cwd": "openidconnect\\example",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "args": [
        "--web-enable-expression-evaluation",
        "--web-port",
        "62568"
      ]
    }
  ]
}